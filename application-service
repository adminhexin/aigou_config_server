spring:
  profiles:
    active:
    - dev

---
spring:
  profiles: dev
  application:
    name: AIGOU_PLAT
eureka:
  client:
    serviceUrl:
      defaultZone: http://127.0.0.1:1000/eureka/
  instance:
    prefer-ip-address: true  #使用ip注册到注册中心
server:
  port: 2111
  
